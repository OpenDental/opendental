<local:FrmODBase x:Class="OpenDental.FrmUserGroupEdit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="468" Height="152" 
             Text="User Group Edit" MinMaxBoxes="False" >
	<Grid Background="#FFFCFDFE" >
		<ui:Button x:Name="butDelete" Width="88" VerticalAlignment="Bottom" Height="26" Margin="22,0,0,21" Text="Delete" Icon="DeleteX" Click="butDelete_Click" />
		<ui:TextBox x:Name="textDescription" Width="206" Height="20" Margin="120,19,0,0" TabIndexOD="0" />
		<ui:Label x:Name="label1" Width="100" Height="16" Margin="16,21,0,0" Text="Description" HAlign="Right" VAlign="Center" />
		<ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="26" Margin="0,0,20,21" Text="Save" Click="butSave_Click" />
	</Grid>
</local:FrmODBase>