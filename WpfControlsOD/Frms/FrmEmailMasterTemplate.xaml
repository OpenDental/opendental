             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="944" Height="753" 
             Text="Email Master Template" Loaded="FrmEmailMasterTemplate_Loaded" >
	<Grid Background="#FFFCFDFE" >
		<ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Margin="0,0,12,12" Text="Save" Click="butSave_Click" />
		<ui:Label x:Name="label1" Width="154" Height="16" Margin="12,16,0,0" Text="Master Page:" VAlign="Bottom" />
		<ui:TextBox x:Name="textMaster" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="12,35,12,51" IsMultiline="True" />
	</Grid>
</local:FrmODBase>