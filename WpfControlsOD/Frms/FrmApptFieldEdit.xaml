<local:FrmODBase x:Class="OpenDental.FrmApptFieldEdit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="375" Height="180" 
             Text="Edit Appointment Field" MinMaxBoxes="False">
    <Grid Background="#FFFCFDFE" >
        <ui:TextBox x:Name="textValue" Width="333" Height="74" Margin="21,40,0,0" IsMultiline="True" TabIndexOD="0"/>
        <ui:Label x:Name="labelName" Width="335" Height="20" Margin="19,17,0,0" Text="Field Name" VAlign="Bottom" />
        <ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="25" Margin="0,0,21,17" Text="_Save" Click="butSave_Click" />
    </Grid>
</local:FrmODBase>