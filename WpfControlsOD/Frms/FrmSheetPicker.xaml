<local:FrmODBase x:Class="OpenDental.FrmSheetPicker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="458" Height="436" 
             Text="Pick Sheet" Loaded="FrmSheetPicker_Loaded" >
	<Grid Background="#FFFCFDFE" >
		<ui:Label x:Name="labelTerminal" HorizontalAlignment="Right" Width="88" Height="30" Margin="0,205,24,0" Text="Multiple sheets can be sent" />
		<ui:Button x:Name="butTerminal" HorizontalAlignment="Right" Width="75" Height="24" Margin="0,175,35,0" Text="To Kiosk" Click="butTerminal_Click" />
		<ui:Label x:Name="label2" Width="301" Height="40" Margin="29,9,0,0" Text="Sheets can be added and edited from Setup Sheets." />
		<ui:Label x:Name="labelSheetType" Width="239" Height="14" Margin="29,62,0,0" Text="Patient Forms and Medical Histories" VAlign="Bottom" />
		<ui:ListBox x:Name="listMain" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="30,81,128,26" MouseDoubleClick="listMain_DoubleClick"/>
		<ui:Button x:Name="butOK" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Margin="0,0,35,27" Text="_OK" Click="butOK_Click" />
	</Grid>
</local:FrmODBase>