<local:FrmODBase x:Class="OpenDental.FrmZipSelect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="430" Height="222" 
             Text="Select Zipcode" MinMaxBoxes="False" >
	<Grid Background="#FFFCFDFE" >
		<ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="26" Margin="0,0,14,12" Text="_Save" Click="butSave_Click" />
		<ui:Button x:Name="butEdit" Width="75" VerticalAlignment="Bottom" Height="26" Margin="203,0,0,12" Text="_Edit" Click="butEdit_Click" BitmapFileName="editPencil.gif" />
		<ui:Button x:Name="butDelete" Width="75" VerticalAlignment="Bottom" Height="26" Margin="114,0,0,12" Text="_Delete" Icon="DeleteX" Click="butDelete_Click" />
		<ui:Button x:Name="butAdd" Width="75" VerticalAlignment="Bottom" Height="26" Margin="25,0,0,12" Text="_Add" Icon="Add" Click="butAdd_Click" />
		<ui:Label x:Name="label1" Width="377" Height="16" Margin="25,23,0,0" Text="Cities attached to this zipcode:" VAlign="Bottom" />
		<ui:ListBox x:Name="listMatches" Width="197" Height="116" Margin="24,45,0,0"/>
	</Grid>
</local:FrmODBase>