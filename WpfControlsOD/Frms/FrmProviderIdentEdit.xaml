<local:FrmODBase x:Class="OpenDental.FrmProviderIdentEdit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="483" Height="269" 
             Text="Edit Provider Identification" Loaded="FrmProviderIdentEdit_Loaded" >
	<Grid Background="#FFFCFDFE" >
		<ui:TextBox x:Name="textIDNumber" Width="90" Height="20" Margin="162,143,0,0" />
		<ui:Label x:Name="label3" Width="145" Height="19" Margin="14,145,0,0" Text="Assigned ID Number" HAlign="Right" VAlign="Center" />
		<ui:TextBox x:Name="textPayorID" Width="90" Height="20" Margin="162,20,0,0" />
		<ui:Label x:Name="label2" Width="145" Height="19" Margin="14,22,0,0" Text="Payor (Electronic) ID" HAlign="Right" VAlign="Center" />
		<ui:Label x:Name="label1" Width="118" Height="19" Margin="41,42,0,0" Text="Type" HAlign="Right" VAlign="Center" />
		<ui:ListBox x:Name="listType" Width="120" Height="95" Margin="162,44,0,0" />
		<ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="26" Margin="0,0,26,23" Text="Save" Click="butSave_Click" />
	</Grid>
</local:FrmODBase>