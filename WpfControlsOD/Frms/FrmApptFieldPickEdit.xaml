<local:FrmODBase x:Class="OpenDental.FrmApptFieldPickEdit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="375" Height="538" 
             Text="Edit Appointment Field Pick List" MinMaxBoxes="False">
    <Grid Background="#FFFCFDFE" >
        <ui:ListBox x:Name="listBoxPick" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="22,40,20,52" />
        <ui:Label x:Name="labelName" Width="335" Height="20" Margin="19,17,0,0" Text="Field Name" VAlign="Bottom" />
        <ui:Button x:Name="butOK" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Margin="0,0,21,10" Text="_OK" Click="butOK_Click" />
    </Grid>
</local:FrmODBase>