<local:FrmODBase x:Class="OpenDental.FrmInsSelectSubscr"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="420" Height="263" 
             Text="Select Insurance Plan" Loaded="FrmInsSelectSubscr_Loaded" >
	<Grid Background="#FFFCFDFE" >
		<ui:Button x:Name="butOK" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="26" Margin="0,0,28,26" Text="_OK" Click="butOK_Click" />
		<ui:Button x:Name="butNew" Width="85" VerticalAlignment="Bottom" Height="26" Margin="26,0,0,26" Text="New Plan" Click="butNew_Click" />
		<ui:ListBox x:Name="listPlans" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="24,21,28,82" MouseDoubleClick="listPlans_DoubleClick"/>
	</Grid>
</local:FrmODBase>